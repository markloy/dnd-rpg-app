# .env file
# This file contains environment variables for our React application
# Environment variables are settings that can change between development, testing, and production

# REACT_APP_ prefix is REQUIRED for React to read the variable
# This tells our frontend where our backend API is running
REACT_APP_API_URL=https://localhost:7043/api

# Enable debug mode (true/false)
REACT_APP_DEBUG=true

# App version for display purposes
REACT_APP_VERSION=1.0.0